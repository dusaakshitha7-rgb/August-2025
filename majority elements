class Solution: 
    def majorityElement(self, nums: List[int]) -> int:
        n = len(nums)
        d = {}  # Dictionary to store frequency of each number
        for i in nums:
            if i in d:
                d[i] += 1
            else:
                d[i] = 1
        # Now check which number appears more than n//2 times
        for i, j in d.items():
            if j > n // 2:
                return i
